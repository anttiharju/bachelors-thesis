<w:t xml:space="preserve">Standish Group Chaos Reports: </w:t>
</w:r>
</w:p>
<w:p w14:paraId="58C9127F" w14:textId="077F66DF" w:rsidR="00EA36F3" w:rsidRDefault="00EA36F3" w:rsidP="00EA36F3">
